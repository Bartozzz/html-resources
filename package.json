{
    "name"          : "html-resources",
    "version"       : "0.0.1",
    "keywords"      : [ "web", "resources", "assets", "html", "parser" ],
    "description"   : "Parses .html files for custom resources",

    "author"        : "≈Åaniewski Bartosz <laniewski.bartozzz@gmail.com> (http://laniewski.me/)",
    "license"       : "MIT",

    "main"          : "dist/index.js",

    "repository" : {
        "type" : "git",
        "url"  : "https://github.com/Bartozzz/html-resources.git"
    },

    "bugs" : {
        "url" : "https://github.com/Bartozzz/html-resources/issues"
    },

    "dependencies" : {
        "merge-descriptors"                 : "^1.0.1",
        "cheerio"                           : "^0.20.0"
    },

    "devDependencies" : {
        "mocha"                             : "^2.5.3",
        "babel"                             : "^6.5.2",
        "babel-cli"                         : "^6.10.1",
        "babel-preset-es2015"               : "^6.9.0",
        "babel-preset-stage-0"              : "^6.5.0",
        "babel-plugin-add-module-exports"   : "^0.2.1"
    },

    "scripts" : {
        "test"       : "mocha --harmony",
        "clean"      : "rm -rf dist",
        "build"      : "babel src -d dist",
        "watch"      : "babel src -d dist -w",
        "prepublish" : "npm run clean && npm run build"
